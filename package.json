{
  "name": "mywebterm",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun --hot src/index.ts --port 18671",
    "build": "bun build ./src/index.html --outdir=dist --sourcemap --target=browser --minify --define:process.env.NODE_ENV='\"production\"'",
    "start": "NODE_ENV=production bun --no-env-file src/index.ts --port 8671",
    "lint": "bunx biome check src/",
    "lint:fix": "bunx biome check --write src/",
    "tsc": "bunx tsc --noEmit",
    "build:tray": "mkdir -p dist && swiftc -framework Cocoa -O -o dist/MyWebTermTray src/tray/tray.swift",
    "build:app": "scripts/build-app-bundle.sh"
  },
  "dependencies": {
    "@xterm/addon-fit": "0.12.0-beta.156",
    "@xterm/xterm": "6.1.0-beta.156",
    "react": "^19",
    "react-dom": "^19",
    "sonner": "^2.0.7"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.15",
    "@types/bun": "^1.3.9",
    "@types/react": "^19",
    "@types/react-dom": "^19"
  }
}
